.slider-inner {
  overflow: hidden;
}
.slides {
  width: 800%;
  transition: all 1s;
}
.slides img {
  width:1030px;
  height: 459px;
  float: left;
}
#btn1:checked~slider-inner slides {
  transform: translate(0);
}

#btn2:checked~.slider-inner .slides {
  transform: translate(-1030px);
}

