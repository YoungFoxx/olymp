.slider {
  position:relative;
  top:0;
  bottom:0;
  width: 100%;
}
.slider input[name="switch"] {
  display: none;
}
.switch {
  text-align: center;
  width: 100%;
}
.switch label {
  display: inline-block;
  width: 13px;
  height: 13px;
  cursor: pointer;
  margin: 0 0px;
  box-shadow: 0 0 2px 0 rgba(0, 0, 0, .8);
  border-radius: 50%;
  border: 1px solid rgba(0, 120, 201, 0.4);
  background-color: rgba(0, 120, 201, 0.4);
}

#btn1:checked~.switch label[for="btn1"] {
  background-color: #6BE400	;
}

#btn2:checked~.switch label[for="btn2"] {
